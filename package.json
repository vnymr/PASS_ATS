{
  "name": "resume-generator",
  "version": "1.0.0",
  "description": "Resume Generator with Chrome Extension and Backend Server",
  "scripts": {
    "start": "cd server && npm start",
    "start:local": "./start-local.sh",
    "dev": "cd server && npm run dev",
    "install:server": "cd server && npm install",
    "build:extension": "./build-extension.sh",
    "setup": "npm run install:server",
    "db:migrate": "cd server && npx prisma migrate dev",
    "db:push": "cd server && npx prisma db push",
    "db:generate": "cd server && npx prisma generate"
  },
  "keywords": [
    "resume",
    "generator",
    "chrome-extension"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.12.2",
    "eventsource": "^4.0.0"
  }
}
